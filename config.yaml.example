logging: debug

mqtt:
  broker: "127.0.0.1"
  port: 1883
  user: 
  password:

components:
  - platform: macos_notify
    state_topic: mqtthawk/notify
    
  - platform: macos_brightness
    command_topic: mqtthawk/brightness/set
    state_topic: mqtthawk/brightness

  - platform: keystroke
    topic: mqtthawk/keystroke

  - platform: command_line
    command_topic: mqtthawk/command/set
    state_topic: mqtthawk/command

  - platform: siri
    topic: mqtthawk/siri
